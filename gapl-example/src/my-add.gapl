function map(parameter operation: function wire[32] => wire[32]) i: wire[32] => o: wire[32]
{
    i => function operation => o;
}

function double() i: wire[32] => o: wire[32]
{
    i, i => function add() => o;
}

function main() i: wire[32] => o: wire[32]
{
    i => function map(function double()) => o;
}
