function unary() i: wire[32] => o: wire[32] {
    i[0:15], i[16:31] => multiply(16) => o[0:15];
    i[0:15], i[16:31] => add(16)      => o[16:31];
}

function test() i: wire[32] => o: wire[32] {
    i => register(wire[32])
        => unary()
        => unary()
        => unary()
        => unary()
        => o;
}
